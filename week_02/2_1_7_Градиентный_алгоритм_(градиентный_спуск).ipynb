        "# f = lambda x: .5 * x + .2 * x**2 -.1 * x**3\n",
        "df = lambda x: -.3 * x**2 + .4 * x + .5\n",
        "\n",
        "nu = 0.01\n",
        "x = -4\n",
        "N = 200\n",
        "\n",
        "for _ in range(N):\n",
        "    x -= nu * df(x)\n",
        "x"
